[
  {
    "context": "Workspace",
    "bindings": {
      "MODKEY-g": "git_panel::ToggleFocus",
      "MODKEY-e": "project_panel::ToggleFocus",
      "MODKEY-i": "assistant::ToggleFocus",
      "MODKEY-`": "terminal_panel::ToggleFocus",
      "MODKEY-w": "pane::CloseActiveItem",
      "ctrl-h": ["workspace::ActivatePaneInDirection", "Left"],
      "ctrl-l": ["workspace::ActivatePaneInDirection", "Right"],
      "ctrl-k": ["workspace::ActivatePaneInDirection", "Up"],
      "ctrl-j": ["workspace::ActivatePaneInDirection", "Down"]
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "MODKEY-w": "pane::CloseActiveItem",
      "shift-MODKEY-w": "pane::CloseActiveItem",
      "ctrl-h": ["workspace::ActivatePaneInDirection", "Left"],
      "ctrl-l": ["workspace::ActivatePaneInDirection", "Right"],
      "ctrl-k": ["workspace::ActivatePaneInDirection", "Up"],
      "ctrl-j": ["workspace::ActivatePaneInDirection", "Down"],
      "shift-MODKEY-i": "assistant::InlineAssist"
    }
  }
]
